<%= form_for(@receipt) do |f| %>

<%= f.label :tray_id %>
<%= f.select(:tray_id, current_user.trays.all.collect {|t| [ t.name, t.id ] })%>

<%= f.label :transaction_date %>
<%= f.text_field :transaction_date %>

<%= f.label :amount %>
<%= f.text_field :amount %>

<%= f.label :image %>
<%= f.file_field :image %>

 <div class="actions">
    <%= f.submit %>
  </div>

<% end %>